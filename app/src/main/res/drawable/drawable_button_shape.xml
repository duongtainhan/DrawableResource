<?xml version="1.0" encoding="utf-8"?>
<shape
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <size
        android:width="200dp"
        android:height="200dp"/>
    <solid
        android:color="#f20"/>
    <padding
        android:bottom="5dp"
        android:left="5dp"
        android:right="5dp"
        android:top="5dp"/>
    <corners
        android:radius="100dp"/>
    <stroke
        android:width="2dp"
        android:color="#164d87"
        android:dashGap="20dp"
        android:dashWidth="50dp"/>
    <gradient
        android:angle="135"
        android:centerColor="#0ff"
        android:startColor="#49de36"
        android:endColor="#726ce9"/>
</shape>